<apex:page showHeader="false" sidebar="false"  docType="html-5.0" applyBodyTag="false" applyHtmlTag="false" renderAs="pdf"  standardController="Quote" extensions="SubscriptionWithOTIOFController">
    <head>
        
        <style>
            
            @page {
            background-image: url({!WaterMarkisTrue});
            background-repeat: no-repeat;
            background-position: center;
            margin-top: 1cm;
            margin-right:1cm;
            }
            
            .header {
            position: running(header);
            width:100%;
            margin-bottom:50px;
            }
            @page {
            margin-top: 150px;padding:0px;margin-bottom: 90px;
            @top-right {
            content: element(header);
            
            }
            }
            .pageStart{
            margin-top:0px;
            }
            .brandfontcolor{
            color:#00b26f;
            font-size:25px;
            font-weight: bold;
            }
            table {
            border-collapse: collapse;
            width: 100%;
            }
            
            th, td {
            border: 1px solid black;
            padding: 8px;
            text-align: left;
            }
        </style>
    </head>
    <body>
        <apex:variable var="OTIPaymentMilestone" value="100% payable on Order Form signing" />
        <div >
            <div class="header">
                <img src="{!$Resource.heaserlogo}" style='float:right;width:400px;'/>
            </div>
        </div>
        
        <div style='margin-top:500px;background:#00b26f;width:35%'>
            <b style="color:White;font-size:35px;font-weight: bold;">ORDER FORM</b>
        </div>
        
        <div style="page-break-after: always"/>
        <div class="pageStart ">
            <p class="brandfontcolor">
                Happay Order Form
            </p>
            <div>
                <table style="width:100%;border:1px solid black;border-collapse:collapse;">
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Proposal Reference Number</td>
                        <td style="border:1px solid black;padding:10px;">{!dataWrapperObj.quoteDetails.Name}</td>
                    </tr>
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Billing contact Name</td>
                        <td style="border:1px solid black;padding:10px;">{!dataWrapperObj.quoteDetails.Contact.Name}</td>
                    </tr>
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Billing contact Email</td>
                        <td style="border:1px solid black;padding:10px;">{!dataWrapperObj.quoteDetails.Contact.Email}</td>
                    </tr>
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Proposed By</td>
                        <td style="border:1px solid black;padding:10px;">{!$User.FirstName} {!$User.LastName}</td>
                    </tr>
                </table>
            </div>
            <div style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;;">
                
            </div>
            <p style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;">
                <span style="padding-left:30px;font-size:20px;padding-top:30px;padding-bottom:30px;font-weight: bold;">SERVICE PROVIDER DETAILS</span>
            </p>
            <div>
                <b style="font-size:12px;font-family: Noto Sans,Helvetica,sans-serif;">VA TECH VENTURES PRIVATE LIMITED</b> <br/>
                <br/> <span style="">3rd Floor, Delta Block, Sigma Soft-Tech</span> 
                <br/> <span style="">Park, No.7 Whitefield Main Road, </span>
                <br/> <span style="">Ramagodanahalli, Varthur Hobli, </span>
                <br/> <span style="">Bangalore 560066</span>
            </div>
            <br/>
            <div style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;;">
                
            </div>
            <p style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;">
                <span style="padding-left:30px;font-size:20px;padding-top:30px;padding-bottom:30px;font-weight: bold;">CLIENT DETAILS</span>
            </p>
            <div>
                <table style="width:100%;border:1px solid black;border-collapse:collapse;">
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Client Entity Name</td>
                        <td style="border:1px solid black;padding:10px;">{!dataWrapperObj.quoteDetails.Account.Name}</td>
                    </tr>
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Registered Address</td>
                        <td style="border:1px solid black;padding:10px;">{!dataWrapperObj.quoteDetails.Account.BillingStreet}, {!dataWrapperObj.quoteDetails.Account.BillingCity} {!dataWrapperObj.quoteDetails.Account.BillingState}  {!dataWrapperObj.quoteDetails.Account.BillingPostalCode}, {!dataWrapperObj.quoteDetails.Account.BillingCountry}</td>
                    </tr>
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">CIN / Registration Number</td>
                        <td style="border:1px solid black;padding:10px;">{!dataWrapperObj.quoteDetails.Account.CIN__c}</td>
                    </tr>
                </table>
            </div>
            <div style="margin-top:3px;">
                <div style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;;">
                    
                </div>
                <p style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;">
                    <span style="padding-left:30px;font-size:20px;padding-top:30px;padding-bottom:30px;font-weight: bold;">SOLUTION OFFERED</span>
                </p>
                <div>
                    <table style="width:100%;border:1px solid black;border-collapse:collapse;">
                        <tr>
                            <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Happay Offerings</td>
                            <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && dataWrapperObj.addOnModules && dataWrapperObj.subscriptionTravelQLIListSize != null)}">
                                <td style="border:1px solid black;padding:10px;">Travel and Expense + AddOn</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && dataWrapperObj.subscriptionTravelQLIListSize != null && !dataWrapperObj.addOnModules)}">
                                <td style="border:1px solid black;padding:10px;">Travel and Expense</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && dataWrapperObj.addOnModules && dataWrapperObj.subscriptionTravelQLIListSize == null)}">
                                <td style="border:1px solid black;padding:10px;">Expense + AddOn</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && !dataWrapperObj.addOnModules && dataWrapperObj.subscriptionTravelQLIListSize == null)}">
                                <td style="border:1px solid black;padding:10px;">Expense</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize == null && dataWrapperObj.subscriptionTravelQLIListSize != null)}">
                                <td style="border:1px solid black;padding:10px;">Travel</td>
                            </apex:outputText>
                            
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div style="page-break-after: always"/>
        <div class="pageStart">
            <div style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;;">
                
            </div>
            <p style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;">
                <span style="padding-left:30px;font-size:20px;padding-top:30px;padding-bottom:30px;font-weight: bold;">SUBSCRIPTION SUMMARY</span>
            </p>
            
            <div>
                <table style="width:100%;border:1px solid black;border-collapse:collapse;">
                    <tr>
                        <td colspan="2"  style="border:1px solid black;background-color: #dddddd;padding:10px;">Offered Commercials validity up to <span><apex:outputText value="{0,date,MM/dd/yy}"> <apex:param value="{!dataWrapperObj.quoteDetails.ExpirationDate}" /> </apex:outputText></span></td>
                    </tr>
                    <tr>
                        <td style="border:1px solid black;background-color: #dddddd;padding:10px;width:30%;">Description</td>
                        <td style="border:1px solid black;padding:10px;background-color: #dddddd;">Amounts (INR)</td>
                    </tr>
                    
                    
                    
                    
                    
                    <apex:repeat value="{!dataWrapperObj.termsDetilsList}" var="term">
                        <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'OTI Payment Milestone'}">
                            <apex:variable var="OTIPaymentMilestone" value="{!term.Term_Attribute_Description__c}" />
                        </apex:outputText>
                        <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name != 'OTI Payment Milestone'}">
                            <tr>
                                <apex:outputText rendered="{!(term.Term_Section_Line_Item__r.Name != 'YoY | PUPM/Report 50-100' && 
                                                           term.Term_Section_Line_Item__r.Name != 'YoY | PUPM/Report >100' && 
                                                           term.Term_Section_Line_Item__r.Name != 'YoY | AMC 10-20% of ARR')}">
                                    <td style="border:1px solid black;padding:10px;">{!term.Term_Section_Line_Item__r.Name}</td>
                                </apex:outputText>
                                <apex:outputText rendered="{!(term.Term_Section_Line_Item__r.Name == 'YoY | PUPM/Report 50-100' || 
                                                           term.Term_Section_Line_Item__r.Name == 'YoY | PUPM/Report >100' ||
                                                           term.Term_Section_Line_Item__r.Name == 'YoY | AMC 10-20% of ARR')}">
                                    <td style="border:1px solid black;padding:10px;">YOY Escalation</td>
                                    
                                </apex:outputText>
                                
                                <td style="border:1px solid black;padding:10px;">
                                    
                                    <apex:outputText rendered="{!(term.Term_Section_Line_Item__r.Name != 'Delay Payment Interest' && 
                                                               term.Term_Section_Line_Item__r.Name != 'YoY | PUPM/Report 50-100' && 
                                                               term.Term_Section_Line_Item__r.Name != 'YoY | PUPM/Report >100' && 
                                                               term.Term_Section_Line_Item__r.Name != 'YoY | AMC 10-20% of ARR' && 
                                                               term.Term_Section_Line_Item__r.Name != 'OTI Payment Milestone' && 
                                                               term.Term_Section_Line_Item__r.Name != 'SAAS Start Date' && 
                                                               term.Term_Section_Line_Item__r.Name != 'Change Request' &&
                                                               term.Term_Section_Line_Item__r.Name != 'Payment Due Date')}">
                                        {!term.Term_Attribute_Line_Item__r.Name}<br/>
                                        <apex:outputText rendered="{!term.Comments__c !=null}" value="{!term.Comments__c}" escape="false"/>
                                    </apex:outputText>
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'SAAS Start Date'}">
                                        
                                        <apex:outputText value="{0,date,MM/dd/yyyy}">
                                            <apex:param value="{!dataWrapperObj.quoteDetails.SAAS_Start_Date__c}" />
                                        </apex:outputText>
                                    </apex:outputText>
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'Payment Due Date'}">
                                        Net {!term.Term_Attribute_Line_Item__r.Name} Days from the date of invoice
                                    </apex:outputText>
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'Change Request'}">
                                        {!dataWrapperObj.quoteDetails.Change_Request__c}
                                    </apex:outputText>
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'OTI Payment Milestone'}">
                                        <apex:variable var="OTIPaymentMilestone" value="{!term.Term_Attribute_Description__c}" />
                                        <!--apex:outputText rendered="{!term.Term_Attribute_Description__c !=null}">{!term.Term_Attribute_Description__c}</apex:outputText>
<apex:outputText rendered="{!term.Comments__c !=null}" value="{!term.Comments__c}" escape="false"/-->
                                    </apex:outputText>
                                    
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'Delay Payment Interest'}">
                                        <apex:outputText rendered="{!term.Late_Payment_Interest__c !=null}">{!term.Late_Payment_Interest__c} %</apex:outputText>
                                        <apex:outputText rendered="{!term.Comments__c !=null}" value="{!term.Comments__c}" escape="false"/>
                                    </apex:outputText>
                                    
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'YoY | PUPM/Report 50-100'}">
                                        <apex:outputText rendered="{!term.YoY_PUPM_Report__c !=null}">{!term.YoY_PUPM_Report__c} %</apex:outputText>
                                        <apex:outputText rendered="{!term.Comments__c !=null}" value="{!term.Comments__c}" escape="false"/>
                                    </apex:outputText>
                                    
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'YoY | PUPM/Report >100'}">
                                        <apex:outputText rendered="{!term.YoY_PUPM_Report_100__c !=null}">{!term.YoY_PUPM_Report_100__c} %</apex:outputText>
                                        <apex:outputText rendered="{!term.Comments__c !=null}" value="{!term.Comments__c}" escape="false"/>
                                    </apex:outputText>
                                    
                                    <apex:outputText rendered="{!term.Term_Section_Line_Item__r.Name == 'YoY | AMC 10-20% of ARR'}">
                                        <apex:outputText rendered="{!term.YoY_AMC_10_20_of_ARR__c !=null}">{!term.YoY_AMC_10_20_of_ARR__c} %</apex:outputText>
                                        <apex:outputText rendered="{!term.Comments__c !=null}" value="{!term.Comments__c}" escape="false"/>
                                    </apex:outputText>
                                </td>
                            </tr>
                        </apex:outputText>
                        
                    </apex:repeat>
                </table>
            </div>
        </div>
        
        <div style="page-break-after: always"/>
        <apex:outputText rendered="{!dataWrapperObj.subscriptionExpenseQLIListSize != null}">
            <div class="pageStart">
                <apex:variable var="index" value="{!0}" />
                <div style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;;">
                    
                </div>
                <p style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;">
                    <span style="padding-left:30px;font-size:20px;padding-top:30px;padding-bottom:30px;font-weight: bold;">SUBSCRIPTION FEES</span>
                </p>
                
                <table>
                    <tr>
                        <th Style ="width:25">Product Name</th>
                        <th>Pricing Type</th>
                        <th>Quantity</th>
                        <th>Total Price</th>
                    </tr>
                    
                    
                    <apex:repeat value="{!dataWrapperObj.subscriptionExpenseQLIList}" var="subOLI">
                        <tr>
                            <apex:outputText rendered="{!dataWrapperObj.addOnModules && subOLI.Product2.Name == 'Modules in Standard'}">
                                <td>{!subOLI.Product2.Name} + Addon</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!!dataWrapperObj.addOnModules && subOLI.Product2.Name == 'Modules in Standard'}">
                                <td>{!subOLI.Product2.Name}</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!(subOLI.Product2.Name != 'Modules in Standard' && subOLI.Product2.Name != 'Route Tracker')}">
                                <td>{!subOLI.Product2.Name}</td>
                            </apex:outputText>
                            <apex:outputText rendered="{!subOLI.Product2.Name == 'Route Tracker'}">
                                <td>{!subOLI.Product2.Name} ({!subOLI.Sub_Type__c})</td>
                            </apex:outputText>
                            
                            <apex:outputPanel layout="none" rendered="{!index == 0}">
                                <td rowspan="{!dataWrapperObj.subscriptionExpenseQLIListSize}">{!subOLI.Travel_Pricing_Type__c}</td>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none" rendered="{!index == 0}">
                                <td rowspan="{!dataWrapperObj.subscriptionExpenseQLIListSize}">{!dataWrapperObj.quoteDetails.Expense_Minimum_Commitment__c}</td>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none" rendered="{!index == 0}" >
                                <td rowspan="{!dataWrapperObj.subscriptionExpenseQLIListSize}">
                                    <apex:outputText value="{0, number,##,##,###,##0.00}">
                                        
                                        <apex:param value="{!dataWrapperObj.woWebChargePricing}" />
                                        
                                    </apex:outputText>
                                </td>
                            </apex:outputPanel>
                            
                        </tr>
                        
                        <apex:variable var="index" value="{!index + 1}" />
                    </apex:repeat>
                    <apex:outputPanel layout="none" rendered="{!dataWrapperObj.webCharges != null}" >
                        <tr>
                            <td>Web Services Charge</td>
                            <td>{!dataWrapperObj.webServiceChargeQLI.Travel_Pricing_Type__c}</td>
                            <td>{!dataWrapperObj.webServiceChargeQLI.SAAS_Quantity__c}</td>
                            <td>
                                <apex:outputText value="{0, number,##,##,###,##0.00}">
                                    
                                    <apex:param value="{!dataWrapperObj.webCharges}" />
                                    
                                </apex:outputText>
                            </td>
                        </tr>
                    </apex:outputPanel>
                    
                    <tr>
                        <td ><b>Expense Monthly Price</b></td>
                        <td colspan="3" style="text-align:center" >
                            <b>
                                <apex:outputText value="{0, number,##,##,###,##0.00}">
                                    
                                    <apex:param value="{!dataWrapperObj.ExpenseMonthlyAmount}" />
                                    
                                </apex:outputText></b>
                        </td>
                    </tr>
                    
                    
                </table>
                
            </div>
            <div style="page-break-after: always"/>
        </apex:outputText>
        
        <div class="pageStart">
            <apex:outputText rendered="{!dataWrapperObj.subscriptionExpenseQLIListSize == null}">
                <p style=" border-left-style: solid; border-width: 7px;  border-color: #00b26f;width:100%;">
                    <span style="padding-left:30px;font-size:20px;padding-top:30px;padding-bottom:30px;font-weight: bold;">SUBSCRIPTION FEES</span>
                </p>
            </apex:outputText>
            <apex:variable var="index" value="{!0}" />
            <apex:outputText rendered="{!dataWrapperObj.subscriptionTravelQLIListSize != null}">
                <table>
                    <tr>
                        <th Style ="width:25">Product Name</th>
                        <th>Pricing Type</th>
                        <th>Quantity</th>
                        <th>Total Price</th>
                    </tr>
                    <apex:repeat value="{!dataWrapperObj.subscriptionTravelQLIList}" var="subOLI">
                        <tr>
                            <td>{!subOLI.Product2.Name}</td>
                            <apex:outputPanel layout="none" rendered="{!index == 0}">
                                <td rowspan="{!dataWrapperObj.subscriptionTravelQLIListSize}">{!subOLI.Travel_Pricing_Type__c}</td>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none" rendered="{!index == 0}">
                                <td rowspan="{!dataWrapperObj.subscriptionTravelQLIListSize}">{!dataWrapperObj.quoteDetails.Travel_Minimum_Commitment__c}</td>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none" rendered="{!index == 0}">
                                <td rowspan="{!dataWrapperObj.subscriptionTravelQLIListSize}">
                                    
                                    <apex:outputText value="{0, number,##,##,###,##0.00}">
                                        
                                        <apex:param value="{!dataWrapperObj.travelPriceWithoutCancelation}" />
                                        
                                    </apex:outputText>
                                </td>
                            </apex:outputPanel>
                            
                        </tr>
                        
                        <apex:variable var="index" value="{!index + 1}" />
                    </apex:repeat>
                    <apex:repeat value="{!dataWrapperObj.subscriptionTravelCancelation}" var="subOLI">
                        <tr>
                            <td> {!subOLI.Product2.Name}</td>
                            <td>{!subOLI.Travel_Pricing_Type__c}</td>
                            <td>{!subOLI.Quantity}</td>
                            <td>
                                <apex:outputText value="{0, number,##,##,###,##0.00}">
                                    
                                    <apex:param value="{!subOLI.SAAS_Price__c}" />
                                    
                                </apex:outputText>
                            </td>
                        </tr>
                    </apex:repeat>
                    <tr>
                        <td ><b>Travel Monthly Price</b></td>
                        <td colspan="3" style="text-align:center" >
                            <b>
                                <apex:outputText value="{0, number,##,##,###,##0.00}">
                                    
                                    <apex:param value="{!dataWrapperObj.quoteDetails.Travel_Total_SAAS__c}" />
                                    
                                </apex:outputText>
                            </b>
                            
                            
                        </td>
                    </tr>
                    <tr>
                        <td colspan="4">
                            Cancellation Charges: Any cancellation will be charged at 40 INR per cancellation.
                        </td>
                    </tr>
                </table>
            </apex:outputText>
            
            
            
            
            
        </div>
        <div style="page-break-after: always"/>
        <div class="pageStart">
            
            <table>
                <apex:outputText rendered="{!dataWrapperObj.totalMonthlypricing != null}">
                    <tr>
                        <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && dataWrapperObj.addOnModules && dataWrapperObj.subscriptionTravelQLIListSize != null)}">
                            <td><b>Total Monthly Price(Travel + Expense + Addon)</b></td>
                        </apex:outputText>
                        <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && dataWrapperObj.subscriptionTravelQLIListSize != null && !dataWrapperObj.addOnModules)}">
                            <td><b>Total Monthly Price(Travel + Expense)</b></td>
                        </apex:outputText>
                        <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && dataWrapperObj.addOnModules && dataWrapperObj.subscriptionTravelQLIListSize == null)}">
                            <td><b>Total Monthly Price(Expense + Addon)</b></td>
                        </apex:outputText>
                        <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize != null && !dataWrapperObj.addOnModules && dataWrapperObj.subscriptionTravelQLIListSize == null)}">
                            <td><b>Total Monthly Price(Expense)</b></td>
                        </apex:outputText>
                        <apex:outputText rendered="{!(dataWrapperObj.subscriptionExpenseQLIListSize == null && dataWrapperObj.subscriptionTravelQLIListSize != null)}">
                            <td><b>Total Monthly Price(Travel)</b></td>
                        </apex:outputText>
                        
                        <td><b>
                            <apex:outputText value="{0, number,##,##,###,##0.00}">
                                
                                <apex:param value="{!dataWrapperObj.totalMonthlypricing}" />
                                
                            </apex:outputText>
                            </b>
                            
                        </td>
                    </tr>
                </apex:outputText>
                
                <apex:outputText rendered="{!(dataWrapperObj.quoteDetails.Overall_ARR_Discount__c != null && dataWrapperObj.quoteDetails.Overall_ARR_Discount__c > 0)}">
                    <tr>
                        <td><b>Discount</b></td>
                        <td>
                            <b><apex:outputText value="{0, number,##,##,###,##0.00}">
                                
                                <apex:param value="{!dataWrapperObj.quoteDetails.Overall_ARR_Discount__c}" /> 
                                
                                </apex:outputText>%</b>
                            
                        </td>
                    </tr>
                </apex:outputText>
                
                <apex:outputText rendered="{!dataWrapperObj.subscriptionExpenseQLIListSize != null}">
                    <tr>
                        <td><b>ARR Expense</b></td>
                        <td><b>
                            <apex:outputText value="{0, number,##,##,###,##0.00}">
                                
                                <apex:param value="{!dataWrapperObj.quoteDetails.Expense_FInal_ARRs__c}" />
                                
                            </apex:outputText>
                            </b>
                            
                        </td>
                    </tr>
                </apex:outputText>
                
                <apex:outputText rendered="{!(dataWrapperObj.quoteDetails.Travel_SAAS_ARR__c != null && dataWrapperObj.quoteDetails.Travel_SAAS_ARR__c > 0)}">
                    <tr>
                        <td><b>ARR Travel</b></td>
                        <td><b>
                            <apex:outputText value="{0, number,##,##,###,##0.00}">
                                
                                <apex:param value="{!dataWrapperObj.quoteDetails.Travel_SAAS_ARR__c}" />
                                
                            </apex:outputText>
                            </b>
                            
                        </td>
                    </tr>
                </apex:outputText>
                
                <apex:outputText rendered="{!dataWrapperObj.subscriptionACV != null}">
